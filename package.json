{
  "name": "rayfraction",
  "version": "0.0.1",
  "description": "The application of ray tracing",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "build-web": "npm run build-renderer",
    "start-web-dev": "webpack-dev-server --config webpack.web.dev.config.js",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "Rayfraction",
    "appId": "me.alohayo.rayfraction",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": [
        "AppImage", 
        "deb"
      ]
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GarfieldZHU/Rayfraction"
  },
  "author": {
    "name": "Garfield Zhu (AlohaYo)",
    "email": "Garfield.bupt@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GarfieldZHU/Rayfraction/issues"
  },
  "homepage": "https://github.com/GarfieldZHU/Rayfraction",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.13",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-grid-layout": "^0.17.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webdriverio": "^4.8.7",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "electron": "^9.0.2",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.8.0",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.8.8",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^11.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/node": "^14.18.33",
    "@types/webpack-env": "^1.18.1",
    "antd": "^4.24.13",
    "install": "^0.13.0",
    "npm": "^7.21.0",
    "ramda": "^0.29.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-grid-layout": "^0.18.3",
    "react-redux": "^7.2.9",
    "react-window": "^1.8.10",
    "redux": "^4.2.1"
  }
}
